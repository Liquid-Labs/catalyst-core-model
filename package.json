{
  "name": "@liquid-labs/catalyst-core-model",
  "version": "1.0.0-prototype.0",
  "description": "Catalyst core component models.",
  "main": "dist/index.js",
  "bin": {
    "cloud-sql-proxy.sh": "./bin/cloud-sql-proxy.sh",
    "cloud-sql-service.sh": "./bin/cloud-sql-service.sh",
    "go-dev-server.sh": "./bin/go-dev-server.sh"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist/* && mkdir -p ./dist",
    "build": "catalyst-scripts build",
    "pretest": "catalyst-scripts pretest",
    "test": "catalyst-scripts test",
    "lint": "catalyst-scripts lint",
    "lint-fix": "catalyst-scripts lint-fix",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liquid-Labs/catalyst-core-model.git"
  },
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liquid-Labs/catalyst-core-model/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/catalyst-core-model#readme",
  "catalyst": {
    "provides": [
      {
        "name": "cloud-sql-service",
        "interface-classes": [
          "sql"
        ],
        "platform-types": [
          "local",
          "gcp"
        ],
        "purposes": [
          "dev",
          "test",
          "pre-production",
          "production"
        ],
        "ctrl-scripts": [
          "cloud-sql-proxy.sh",
          "cloud-sql-service.sh"
        ],
        "params-req": [
          "GCP_PROJECT_ID",
          "CLOUDSQL_INSTANCE_NAME",
          "CLOUDSQL_CONNECTION_PROT",
          "CLOUDSQL_CONNECTION_NAME",
          "CLOUDSQL_PROXY_CONNECTION_NAME",
          "CLOUDSQL_SERVICE_ACCT",
          "CLOUDSQL_USER",
          "CLOUDSQL_PASSWORD",
          "CLOUDSQL_DB"
        ],
        "params-opt": [],
        "config-const": {}
      }
    ],
    "requires": []
  },
  "dependencies": {
    "@liquid-labs/cloud-sql-proxy": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@liquid-labs/catalyst-scripts": "^1.0.0-alpha.22"
  }
}
